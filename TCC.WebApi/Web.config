  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory name="NHibernate.TCC">
      <property name="connection.driver_class">NHibernate.Driver.MySqlDataDriver</property>
      <property name="connection.connection_string_name">ConexaoPadrao</property>
      <property name="adonet.batch_size">50</property>
      <property name="show_sql">true</property>
      <property name="dialect">NHibernate.Dialect.MySQL5Dialect</property>
      <property name="command_timeout">300</property>
      <property name="query.substitutions">true 1, false 0, yes 'Y', no 'N'</property>
      <property name="generate_statistics">true</property>
      <property name="current_session_context_class">web</property>
      <property name="format_sql">false</property>
      <property name="use_sql_comments">true</property>
      <mapping assembly="TCC.Dados" />
      <!--<mapping assembly="Argus.DataAccess.NHibernate.Mapping" />-->
      <event type="pre-update">
        <listener class="TCC.InjecaoDeDependencias.Auditoria.AuditoriaEventoListener, TCC.InjecaoDeDependencias" />
      </event>
      <event type="flush">
        <listener class="TCC.InjecaoDeDependencias.Auditoria.PatchedDefaultFlushEventListener, TCC.InjecaoDeDependencias" />
      </event>
      <!--<event type="pre-insert">
        <listener class="TCC.InjecaoDeDependencias.Auditoria.AuditoriaEventoListener, TCC.InjecaoDeDependencias" />
      </event>-->
    </session-factory>
  </hibernate-configuration>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="mssqllocaldb" />
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
</configuration>